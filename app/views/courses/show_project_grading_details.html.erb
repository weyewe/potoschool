
<div class="span9 content">
  
  <%= create_guide("Grading Details", 
        "Details for project: <b>#{@project.title}</b><br />" + 
        "Course : <b>#{@course.name}</b><br />" + 
        "For Student NIM: xxxx").html_safe %>
  
  <%= create_breadcrumb(@breadcrumbs).html_safe %>
  

  
  
  <div class="row-fluid">
    <!-- <div class="span12"> -->
      
      <table cellpadding="0" cellspacing="0" border="0" 
            class="table table-striped table-bordered" id="example">
   
        <thead>
          <tr>
            <th>Approved Image</th>
            <th>Score</th>
            <th>Total Revisions</th>
              
       
          </tr>
        </thead>
        <tbody>
          
          <% @students.each do |student| %>
             <tr>
                <td>xxx</td>

                <% if @projects.length == 0 %>
                  <td>-</td>
                <% else %>
                  <% (1..@projects.length).each do |counter| %>
                    <td id="student_<%=student.id%>-project_<%= @projects[counter-1].id%>">
                    <%= extract_project_submission_result(
                              @course.get_score_for(student, @projects[counter-1]) ,
                              student, 
                              @projects[counter-1] 
                    )  %>
                      <!-- 80   -->
                      </td>
                  <% end %>
                <% end %>
              </tr>
          <% end %>
        </tbody>
      </table>
      
      
      
      
    <!-- </div> -->   <!--/span12-->
  </div><!--/row-->

</div><!--/span-->



<% content_for :additional_js  do %>
 <script type="text/javascript">
   $(document).ready(function(){
     $(".popover-able").popover({
       placement :'left'
     });
   });
 </script>
<% end %>










