
<div class="span9 content">
  
  
  <%= create_guide("Past Projects", 
        "Collection of all past projects in the current academic year.").html_safe %>
  
  <%= create_breadcrumb(@breadcrumbs).html_safe %>
  
 
  
  
  <div class="row-fluid">
    <!-- <div class="span12"> -->
      
      <table cellpadding="0" cellspacing="0" border="0" 
            class="table table-striped table-bordered" id="example">
        <thead>
          <tr>
            <th>Title</th>
            <th>Course</th>
            <th>Subject</th>
            <th>Deadline</th>
            <th>Close</th>
          </tr>
        </thead>
        <tbody>
          <% @projects.each do |project| %>
            <tr id="entry_row_<%= project.id %>">
             
              <td><%= link_to "#{project.title}", select_project_submission_for_grading_path(project) %></td>
              <td><%= project.course.name %>
              <td><%= project.course.subject.code %>
              <td><%= project.deadline_datetime.in_time_zone("Jakarta") %>
              
              <td>
                <%= link_to "Recover", '#', 
                          :class => "btn btn-danger entry-link entry-recover",
                          :id => "entry_#{project.id}" %>
              </td>
              
                
            </tr>
          <% end %>


         
        </tbody>
      </table>
      
      
        <div class=" hidden" id="close-entry">
          <%= form_tag  recover_project_url  , :method => :post  do  %>
            <%= hidden_field_tag 'entry_id' , ''  %>
          <% end %>
        </div>
        
        
      
      <!-- <div class="alert alert-success allert-message">
           <a class="close" href="#">&times;</a>
           <strong>Well done!</strong> You successfully read this important alert message.
         </div> -->
   
      
<!--       <div class="form-actions">
        <%#= hidden_field_tag 'destination_url', 
                subject_subject_teaching_assignments_url(@subject) ,
                 :id => "destination_url" %>

        <button class="btn btn-large btn-primary"
              type="submit" id="save_button">Save changes</button>
      </div> -->


      
      
      
    <!-- </div> -->   <!--/span12-->
  </div><!--/row-->

</div><!--/span-->

