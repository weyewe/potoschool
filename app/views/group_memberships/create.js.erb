$(document).ready(function(){
  
  var $checkbox = $("#provider_<%=  @group_id %>-consumer_<%=  @user_id %>");
  var $row = $checkbox.parents("tr");
  if( $row ){
    console.log("Row does exists ");
    console.log("length is " + $row.length );
  }
  var $td_membership = $(".membership-info", $row );
  
  if( $td_membership ){
     console.log("td_membership does exists, class is  " + $td_membership.attr('class') );
     console.log("the text is " + $td_membership.text()) ;
   }
  
  
  
  <% if @decision == TRUE_CHECK %>
    
    <% group_name = escape_javascript(@group.name) %> 
    console.log("This is a true check: <%= group_name %>");
  // added new shite, change td.membership-info text 
    $td_membership.text('<%= group_name %>');
  <% elsif @decision == FALSE_CHECK %>
    console.log("This is a false check " );
    $td_membership.text("Unregistered");
  <% end %>
  
  
  
  
});
