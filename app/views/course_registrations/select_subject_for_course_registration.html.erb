
<div class="span9 content">
  
  <%= create_guide("Course Registration", 
        "Select the subject for course registration").html_safe %>
  
  <%= create_breadcrumb(@breadcrumbs).html_safe %>
  

  
  
  <div class="row-fluid">
    <!-- <div class="span12"> -->
      
      <table cellpadding="0" cellspacing="0" border="0" 
            class="table table-striped table-bordered" id="example">
        <thead>
          <tr>
            <th>Subject</th>
            <th>Total Courses</th>
            <th>Total Student</th>
          </tr>
        </thead>
        <tbody>
          <% @subjects.each do |subject| %>
            <tr>
              <td><%= link_to subject.name, select_course_for_course_registration_url(subject) %></td>
              <td><%= subject.courses.count%></td>
              <td><%= subject.students_count %></td>
            </tr>
          <% end %>


         
        </tbody>
      </table>
      
      
      
      
    <!-- </div> -->   <!--/span12-->
  </div><!--/row-->

</div><!--/span-->

<%# content_for :additional_js  do %>
 
  <%#= javascript_include_tag "DT.js" %> 
  <%#= javascript_include_tag "DT_bootstrap.js" %> 
<%# end %>










